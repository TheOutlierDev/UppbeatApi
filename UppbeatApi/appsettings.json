{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "UppbeatLibraryAPI",
    "Audience": "UppbeatLibraryAPI",
    "Key": "^3*z%*wv=()#3g3@gjfx4$23xh!8&rojkms9g%d28cfv4-+(dr"
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1m",
        "Limit": 10
      }
    ]
  },
  "IpRateLimitPolicies": {
    "IpRules": []
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Port=5433;Database=uppbeatapi;User Id=uppbeatapp;Password=your_strong_password;"
  }
}
